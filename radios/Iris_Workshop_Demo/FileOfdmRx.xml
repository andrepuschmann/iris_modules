<?xml version="1.0" encoding="utf-8" ?> 

<softwareradio name="Radio1">

	<engine name="pnengine1" class="pnengine">

		<component name="usrpuhdrx1" class="usrpuhdrx">
			<parameter name="args" value=""/>
			<parameter name="rate" value="250000"/>
			<parameter name="frequency" value="5000000000"/>
			<parameter name="gain" value="40"/>
			<parameter name="outputblocksize" value="4096"/>
			<port name="output1" class="output"/>
    		</component>

		<component name="simpleofdmdemod1" class="simpleofdmdemod">
			<parameter name="debug" value="false"/>
			<port name="input1" class="input"/>
			<port name="output1" class="output"/>
		</component>

		<component name="filerawwriter1" class="filerawwriter">
			<parameter name="filename" value="out.txt"/>
			<port name="input1" class="input"/>
		</component>

	</engine>

	<link source="usrpuhdrx1.output1" sink="simpleofdmdemod1.input1" />
	<link source="simpleofdmdemod1.output1" sink="filerawwriter1.input1" />

</softwareradio>

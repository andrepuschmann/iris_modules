<?xml version="1.0" encoding="utf-8" ?> 

<softwareradio name="SourceSink1">

	<engine name="stackengine1" class="stackengine">

		<component name="sourcesink1" class="sourcesink">
			<parameter name="direction" value="down"/>
			<port name="topport1" class="io"/>
			<port name="bottomport1" class="io"/>
    		</component>
    		
    		<component name="example1" class="example">
			<port name="topport1" class="io"/>
			<port name="bottomport1" class="io"/>
		</component>


	</engine>
	
	<engine name="pnengine1" class="pnengine">
	
		<component name="src1" class="source">
			<parameter name="counter" value="5"/>
			<port name="output1" class="output"/>
		</component>
	
	</engine>
	
	<engine name="pnengine2" class="pnengine">
		
		<component name="snk1" class="sink">
			<port name="input1" class="input"/>
		</component>
		
	</engine>

	<link source="sourcesink1.bottomport1" sink="example1.topport1" />
	<link source="example1.bottomport1" sink="snk1.input1" />
	<link source="src1.output1" sink="example1.bottomport1" />

</softwareradio>

<?xml version="1.0" encoding="utf-8" ?> 

<softwareradio name="Radio1">

..<engine name="pnengine1" class="pnengine">

....<component name="filerawreader1" class="filerawreader">
......<parameter name="filename" value="testdata.bin"/>
......<parameter name="blocksize" value="300"/>
......<parameter name="datatype" value="uint8_t"/>
......<port name="output1" class="output"/>
....</component>
....
....<component name="sleeptemplate1" class="sleeptemplate">
......<parameter name="sleeptimems" value="1000"/>
......<port name="input1" class="input"/>
......<port name="output1" class="output"/>
....</component>

....<component name="pskqammod1" class="pskqammod">
......<parameter name="modulationdepth" value="1"/>
......<port name="input1" class="input"/>
......<port name="output1" class="output"/>
....</component>
....
....<component name="channel1" class="channel">
......<parameter name="snr" value="10"/>
......<port name="input1" class="input"/>
......<port name="output1" class="output"/>
....</component>

....<component name="matlab1" class="matlabtemplate">
......<parameter name="scriptname" value="cost_terra_scatter(matlab_input)"/>
......<parameter name="hasOutput" value="false"/>
......<port name="input1" class="input"/>
......<port name="output1" class="output"/>
....</component>

..</engine>

..<link source="filerawreader1.output1" sink="sleeptemplate1.input1" />
..<link source="sleeptemplate1.output1" sink="pskqammod1.input1" />
..<link source="pskqammod1.output1" sink="channel1.input1" />
..<link source="channel1.output1" sink="matlab1.input1" />

</softwareradio>

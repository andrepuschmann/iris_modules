<?xml version="1.0" encoding="utf-8" ?> 

<softwareradio name="Radio1">

	<engine name="pnengine1" class="pnengine">
		<component name="timetx1" class="timestamptx">
			<port name="output1" class="output"/>
		</component>
		
		<component name="worker1" class="worker">
			<port name="input1" class="input"/>
			<port name="output1" class="output"/>
		</component>
	</engine>
		
	<engine name="pnengine2" class="pnengine">	
		<component name="worker2" class="worker">
			<port name="input1" class="input"/>
			<port name="output1" class="output"/>
		</component>

		<component name="timerx1" class="timestamprx">
			<port name="input1" class="input"/>
		</component>
	</engine>
	
	<link source="timetx1.output1" sink="worker1.input1" />
	<link source="worker1.output1" sink="worker2.input1" />
	<link source="worker2.output1" sink="timerx1.input1" />
	
</softwareradio>

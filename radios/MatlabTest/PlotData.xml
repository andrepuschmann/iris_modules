<?xml version="1.0" encoding="utf-8" ?> 

<softwareradio name="Radio1">

	<engine name="pnengine1" class="pnengine">

		<component name="filerawreader1" class="filerawreader">
			<parameter name="filename" value="testdata.bin"/>
			<parameter name="blocksize" value="1024"/>
			<parameter name="datatype" value="complex<float>"/>
			<port name="output1" class="output"/>
		</component>

		<component name="matlab1" class="matlabtemplate">
			<parameter name="scriptname" value="crunch(matlab_input)"/>
			<parameter name="hasOutput" value="true"/>
			<parameter name="passThrough" value="false"/>
			<port name="input1" class="input"/>
			<port name="output1" class="output"/>
		</component>
		
		<component name="filerawwriter1" class="filerawwritertemplate">
		    <parameter name="filename" value="output.bin"/>
		    <port name="input1" class="input"/>
		</component>

	</engine>

	<link source="filerawreader1.output1" sink="matlab1.input1" />
	<link source="matlab1.output1" sink="filerawwriter1.input1" />

</softwareradio>

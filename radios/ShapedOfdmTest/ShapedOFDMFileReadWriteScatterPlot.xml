<?xml version="1.0" encoding="utf-8" ?> 

<softwareradio name="Radio1">

    <engine name="pnengine1" class="pnengine">

      <component name="filerawreader1" class="filerawreader">
        <parameter name="filename" value="testdata.bin"/>
        <parameter name="blocksize" value="1024"/>
        <parameter name="datatype" value="uint8_t"/>
        <port name="output1" class="output"/>
      </component>

      <component name="shapedofdmmod1" class="shapedofdmmod">
        <parameter name="debug" value="false"/>
        <parameter name="numfftbins" value="256"/>
        <parameter name="shapingExtLen" value="110"/>
        <parameter name="bitsPerDataSymbol" value="2"/>
        <port name="input1" class="input"/>
        <port name="output1" class="output"/>
      </component>

     <component name="channel1" class="channel">
        <parameter name="debug" value="false"/>
        <parameter name="freqoffset" value="0.05"/>
        <parameter name="snr" value="20"/>
        <port name="input1" class="input"/>
        <port name="output1" class="output"/>
      </component>

      <component name="shapedofdmdemod1" class="shapedofdmdemod">
        <parameter name="debug" value="false"/>
        <parameter name="shapingExtLen" value="110"/>
        <parameter name="bitsPerDataSymbol" value="2"/>
        <parameter name="scatterplot" value="true"/>
        <port name="input1" class="input"/>
        <port name="output1" class="output"/>
        <port name="output2" class="output"/>
      </component>


      <component name="filerawwriter1" class="filerawwriter">
        <parameter name="filename" value="out.txt"/>
        <port name="input1" class="input"/>
      </component>


      <component name="matlab1" class="matlabtemplate">
        <parameter name="scriptname" value="displayscatterplot(matlab_input)"/>
        <parameter name="hasOutput" value="true"/>
        <parameter name="passThrough" value="false"/>
        <port name="input1" class="input"/>
        <port name="output1" class="output"/>
      </component>

      <component name="filerawwriter2" class="filerawwritertemplate">
        <parameter name="filename" value="equalizedSymbols.bin"/>
        <port name="input1" class="input"/>
      </component>

    </engine>

    <link source="filerawreader1.output1" sink="shapedofdmmod1.input1" />
  <link source="shapedofdmmod1.output1" sink="channel1.input1" />
  <link source="channel1.output1" sink="shapedofdmdemod1.input1" />
  <link source="shapedofdmdemod1.output1" sink="filerawwriter1.input1" />
  <link source="shapedofdmdemod1.output2" sink="matlab1.input1" />
  <link source="matlab1.output1" sink="filerawwriter2.input1" />
  

</softwareradio>
